{
  "name": "nanoclaw",
  "version": "1.0.0",
  "description": "Personal Claude assistant. Lightweight, secure, customizable.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ENVOAK_KEY=$(cat .envoak_key) node /Users/macbook/Dev/Envoak/dist/bin/envoak.js inject tsx src/index.ts",
    "auth": "ENVOAK_KEY=$(cat .envoak_key) node /Users/macbook/Dev/Envoak/dist/bin/envoak.js inject tsx src/discord-auth.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.97.0",
    "better-sqlite3": "^11.8.1",
    "cron-parser": "^5.5.0",
    "discord.js": "^14.18.0",
    "elevenlabs": "^1.59.0",
    "openai": "^4.77.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.0",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20"
  },
  "overrides": {
    "tar": "^7.5.7",
    "glob": "^11.1.0",
    "lodash": "^4.17.21"
